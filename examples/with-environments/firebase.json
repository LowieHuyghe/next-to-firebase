{
  "hosting": [{
    "target": "staging",
    "public": "src/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      "_staging-rewrites_"
    ]
  }, {
    "target": "production",
    "public": "src/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      "_production-rewrites_"
    ]
  }],
  "functions": {
    "source": "src/functions"
  }
}
